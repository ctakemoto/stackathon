{
  "name": "toilt",
  "version": "1.0.0",
  "description": "",
  "main": "server/src/app.js",
  "scripts": {
    "start": "node ./server/src/app.js",
    "serve": "vue-cli-service serve ./client/src/main.js",
    "build-client": "vue-cli-service build ./client/src/main.js",
    "lint-client": "vue-cli-service lint",
    "start-server-dev": "node ./server/node_modules/nodemon/bin/nodemon.js ./server/src/app.js --exec 'npm run lint-server && node'",
    "lint-server": "./server/node_modules/.bin/eslint ./server/src/*.js",
    "seed": "node ./server/scripts/seed.js",
    "deploy": "./server/script/deploy",
    "start-dev": "npm run start-server-dev && npm run serve"
  },
  "keywords": [],
  "author": "Cara Takemoto",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.24",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^6.0.0",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "leaflet": "^1.5.1",
    "mapbox-geocoding": "^0.1.5",
    "mapbox-gl": "^1.1.0-beta.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.8.10",
    "vue": "^2.6.10",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10",
    "nodemon": "^1.19.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 1,
      "no-undef": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
